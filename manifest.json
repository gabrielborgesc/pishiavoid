{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "PhishiAvoid",
  "action": {
    "default_popup": "js/index.html",
    "default_title": "PhishiAvoid"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["js/contentScript.js"]
    }
  ],
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }  
}
